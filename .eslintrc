{
  "root": true,
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-use-before-define": ["error", { "functions": false }],
    "no-nested-ternary": "off"
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "extends": "airbnb"
}
